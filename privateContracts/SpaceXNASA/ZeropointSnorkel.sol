pragma solidity v.0.8;


contract ZeropointSnorkel(mask, wearer, oxygen){
import ZeropointShield;

ZeropointShield(encompass) wearer + GateWayFreshAirBridge;

and/or

ZeropointShield(encompass) wearer,
if underwater,
then Zeropoint(boil) water,
then ZeropointShield(expel) hydrogen,
then ZeropointShield(direct) oxygen to wearer,
else if in space then ZeropointShield(emcompass) wearer + GateWayFreshAirBridge;

}
